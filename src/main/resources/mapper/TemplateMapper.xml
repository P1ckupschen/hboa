<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="generator.mapper.TemplateMapper">

    <resultMap id="BaseResultMap" type="generator.entity.Template">
            <id property="templateId" column="template_id" jdbcType="INTEGER"/>
            <result property="templateName" column="template_name" jdbcType="VARCHAR"/>
            <result property="templateContent" column="template_content" jdbcType="VARCHAR"/>
            <result property="isDeleted" column="is_deleted" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        template_id,template_name,template_content,
        is_deleted
    </sql>
</mapper>
